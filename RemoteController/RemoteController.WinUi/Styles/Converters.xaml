<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:c="using:RemoteController.WinUi.Converters">

    <c:InverseBooleanConverter x:Key="Converter.InverseBoolean" />
    <c:BooleanToVisibilityConverter x:Key="Converter.TrueToVisible" />
    <c:BooleanToVisibilityConverter 
        x:Key="Converter.FalseToVisible"
        TrueVisibility="Collapsed"
        FalseVisibility="Visible" />
    <c:GestureToVirtualKeysConverter x:Key="Converter.GestureToVirtualKeys" />
</ResourceDictionary>
